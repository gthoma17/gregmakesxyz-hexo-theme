<%    function getCircularReplacer() {%>
<%        const seen = new WeakSet()%>
<%        return (key, value) => {%>
<%        if (typeof value === "object" && value !== null) {%>
<%            if (seen.has(value)) {%>
<%                return%>
<%            }%>
<%            seen.add(value)%>
<%        }%>
<%        return value%>
<%        }%>
<%    }%>
<% console.log(`=== Debug Info For {${name}}===`) %>
<% console.log(`typeof ${name} === ${typeof item}`) %>
<% if(item){ %>
    <% if ('object' === typeof item){ %>
        <% console.log(`Object.keys(${name}) === ${Object.keys(item)}`) %>
        <% typeof serializeObject !== 'undefined' && serializeObject && console.log(`JSON.stringify(${name}) === ${JSON.stringify(item, getCircularReplacer())}`) %>
    <% } else { %>
        <% console.log(`${name}.toString()" === ${item.toString()}`) %>
    <% } %>
    <% if ('length' in item){ %>
        <% console.log(`${name}.length === ${item.length}`) %>
    <% } %>
<% } else { %>
    <% console.log(name + " was falsey!!!") %>
<% } %>
